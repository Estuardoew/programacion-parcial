/**
 *
 * @author Estuardo Guerra
 */
  public class Persona {
  String nombre;
  String apellido;
  String dpi;
  int nacimiento;

   
  public Persona(String nombre, String apellido, String dpi, int nacimiento) {
    this.nombre = nombre;
    this.apellido = apellido;
    this.dpi = dpi;
    this.nacimiento = nacimiento;
  }

  public void imprimir(){
    System.out.println("Mi nombre es" + this.nombre+ " " + this.apellido + " mi dpi es "+ this.dpi+" naci en el a√±o " + this.nacimiento);
  }


  public static void main(String[] args) {
     
    Persona persona1 = new Persona("jorge", "Morales", "6925 51452 5143", 2000);
    
    Persona person2 = new Persona("Estuardo", "Guerra", "3321 22228 4528", 2003);
    persona1.imprimir();
    person2.imprimir();
  }
}
